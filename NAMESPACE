useDynLib(jl4R, .registration = TRUE)

exportPattern("^\\.jl")
export(
  jlptr, jlptr_unsafe,
  jl, jl_unsafe, jlrun, jlget, jlset, jlshow,
  jlvector,
  "[.jlvector","[<-.jlvector","[[.jlvector","[[<-.jlvector", as.vector.jlvector,
  jl2R, jl2R.default
)

#S3methods are not viewable inside the R system. If you want this functionality available put it in the previous export

S3method(print,jlvector)
S3method("[",jlvector)
S3method("[<-",jlvector)
S3method("[[",jlvector)
S3method("[[<-",jlvector)
S3method(as.vector,jlvector)

S3method(jl2R,default)
S3method(jl2R,jlPtr)
S3method(jl2R,DataFrame)
